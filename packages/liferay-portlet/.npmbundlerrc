{
  "exclude": {
    "@angular/animations": true,
    "@angular/common": true,
    "@angular/compiler": true,
    "@angular/core": true,
    "@angular/forms": true,
    "@angular/platform-browser": true,
    "@angular/platform-browser-dynamic": true,
    "@angular/router": true,
    "core-js": true,
    "rxjs": true,
    "tslib": true,
    "zone.js": true
  },
  "config": {
    "imports": {
      "polyfills-provider": {
        "/": "5.0.0",
        "tslib": "~1.10.0",
        "zone.js": "~0.10.2"
      },
      "rxjs-provider": {
        "rxjs": "~6.5.4"
      },
      "angular-provider": {
        "/": "5.0.0",
        "@angular/animations": "~8.2.14",
        "@angular/common": "~8.2.14",
        "@angular/compiler": "~8.2.14",
        "@angular/core": "~8.2.14",
        "@angular/forms": "~8.2.14",
        "@angular/platform-browser": "~8.2.14",
        "@angular/platform-browser-dynamic": "~8.2.14",
        "@angular/router": "~8.2.14"
      }
    }
  },
  "create-jar": {
    "output-dir": "dist",
    "output-filename": "liferay-portlet.jar",
    "features": {
      "js-extender": true,
      "web-context": "/liferay-portlet",
      "localization": "features/localization/Language",
      "configuration": "features/configuration.json"
    }
  },
  "dump-report": true,
  "max-parallel-files": 128,
  "verbose": true
}
