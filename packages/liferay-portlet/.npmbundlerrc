{
  "config": {
    "imports": {
      "polyfills-provider": {
        "/": "1.0.0",
        "tslib": "~1.10.0",
        "zone.js": "~0.10.2"
      },
      "rxjs-provider": {
        "rxjs": "~6.5.4"
      },
      "angular-provider": {
        "@angular/animations": "~9.0.0-rc.9",
        "@angular/common": "~9.0.0-rc.9",
        "@angular/compiler": "~9.0.0-rc.9",
        "@angular/core": "~9.0.0-rc.9",
        "@angular/forms": "~9.0.0-rc.9",
        "@angular/platform-browser": "~9.0.0-rc.9",
        "@angular/platform-browser-dynamic": "~9.0.0-rc.9",
        "@angular/router": "~9.0.0-rc.9"
      }
    }
  },
  "create-jar": {
    "output-dir": "dist",
    "features": {
      "js-extender": true,
      "web-context": "/liferay-portlet",
      "localization": "features/localization/Language",
      "configuration": "features/configuration.json"
    }
  },
  "dump-report": true,
  "max-parallel-files": 128,
  "verbose": true
}
