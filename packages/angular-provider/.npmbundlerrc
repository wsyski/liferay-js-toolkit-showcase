{
  "config": {
    "imports": {
      "frontend-js-metal-web": {
        "core-js": ">=2.6.5"
      },
      "polyfills-provider": {
        "tslib": "^1.10.0",
        "zone.js": "~0.9.1"
      },
      "rxjs-provider": {
        "rxjs": "~6.5.3"
      }
    }
  },
  "create-jar": {
    "output-dir": "dist",
    "features": {
      "js-extender": false,
      "web-context": "/angular-provider"
    }
  },
  "dump-report": true,
  "max-parallel-files": 128,
  "verbose": true
}
